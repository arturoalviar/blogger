<%= render partial: 'articles/header' %>
<div class="container">

<div class="wrapper blog-list-container">

<h1 class="article-header" >Articles Tagged with <%= @tag.name %></h1>

<ul>
  <% @tag.articles.each do |article| %>
  <li>
    <%= link_to article.title, article_path(article) %>
    
  </li>
  <% end %>
</ul>
</div>
</div>
