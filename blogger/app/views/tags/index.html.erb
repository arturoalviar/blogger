<%= render partial: 'articles/header' %>
<div class="container">
<div class="wrapper blog-list-container">
  <h1 class="article-header" >All Tags</h1>
  <ul id="tags">
    <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag)  %> |
      <%= link_to "Delete #{tag.name} tag", tag_path(tag),  method: :delete %>
    </li>
    <% end %>
  </ul>
  </div>
</div>
